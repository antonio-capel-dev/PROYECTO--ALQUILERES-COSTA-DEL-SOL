{
  "kind": "collectionType",
  "collectionName": "ubicacions",
  "info": {
    "singularName": "ubicacion",
    "pluralName": "ubicacions",
    "displayName": "Ubicacion"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre",
      "required": true
    },
    "descripcion": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "rentals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rental.rental",
      "mappedBy": "ubicacion"
    },
    "seo": {
      "type": "component",
      "component": "seo.meta-seo",
      "repeatable": false
    }
  }
}
