{
  "kind": "collectionType",
  "collectionName": "rentals",
  "info": {
    "singularName": "rental",
    "pluralName": "rentals",
    "displayName": "Rental"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 120,
      "minLength": 10,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "galeria": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "ubicacion": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ubicacion.ubicacion",
      "inversedBy": "rentals"
    },
    "detalles": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "inmueble.detalles",
      "repeatable": false
    },
    "Servicios": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "inmueble.servicios",
      "repeatable": false
    },
    "seo": {
      "type": "component",
      "component": "seo.meta-seo",
      "repeatable": false
    }
  }
}
