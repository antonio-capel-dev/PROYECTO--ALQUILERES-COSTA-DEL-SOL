{
  "collectionName": "components_seo_meta_seos",
  "info": {
    "displayName": "Meta SEO",
    "description": "Componente reutilizable para metadatos SEO"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "metaDescription": {
      "type": "text",
      "required": true,
      "maxLength": 170
    },
    "canonicalURL": {
      "type": "string"
    },
    "ogTitle": {
      "type": "string",
      "maxLength": 90
    },
    "ogDescription": {
      "type": "text",
      "maxLength": 200
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "robots": {
      "type": "string",
      "default": "index, follow"
    }
  }
}
